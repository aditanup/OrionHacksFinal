{
	"manifest_version": 2,

	"name": "Magic Mirror",
	"description": "Used to check and track unreliable news content",
	"version": "0.1",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Magic Mirror!"
	},

	"permissions": [
		"tabs",
		"activeTab",
		"https://ajax.googleapis.com/",
		"https://www.gstatic.com/charts/loader.js"
	]
}
